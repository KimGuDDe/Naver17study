package day1211;

public class Ex7DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name="Candy";
		int age = 32;
		float height= 163.8f;
		double weight=56.7;
		char blood = 'B';
	
		System.out.printf("이름"+ ":" + "%s\n",name);
		System.out.printf("나이"+":"+"%d\n",age);
		System.out.println("키"+":"+"%d\n",height);
	}

}
