package day1213;

public class Ex8While {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 숫자 n을 입력하면 1부터 n까지의 합계 sum을 구해서 출력하시오
		 * 
		 * 예) 100입력 시
		 * 1부터 100까지 합계는 5050입니다(printf)
		 */
		int n, sum;
		
		System.out.println();
		n=1;
		while(n>=1)
		{
			System.out.print(++n);
		}
		
		
		
		
	}
	

}
